                      </td>
                    </tr>
                  </template>
                </tbody>
              </table>
            </div>
            <div
              class="flex justify-between px-4 py-3 text-xs font-semibold tracking-wide text-gray-500 uppercase border-t dark:border-gray-700 bg-gray-50 sm:grid-cols-9 dark:text-gray-400 dark:bg-gray-800">
              <div class="flex items-center">
                <button
                  class="text-left px-4 py-2 mr-1 text-sm font-medium leading-5 text-white transition-colors duration-150 bg-purple-600 border border-transparent rounded-lg active:bg-purple-600 hover:bg-purple-700 focus:outline-none focus:shadow-outline-purple"
                  onclick="assignLeader()">
                  모임장 지정
                </button>
                <button
                  class="text-left px-4 py-2 ml-1 text-sm font-medium leading-5 text-white transition-colors duration-150 bg-purple-600 border border-transparent rounded-lg active:bg-purple-600 hover:bg-purple-700 focus:outline-none focus:shadow-outline-purple">
                  모임 퇴출
                </button>
              </div>

              <div x-data="{ currentPage : 1 }">
                <span class="flex items-center justify-end">
                  <nav aria-label="Table navigation">
                    <ul class="inline-flex items-center">
                      <li>
                        <button class="px-3 py-1 rounded-md rounded-l-lg focus:outline-none focus:shadow-outline-purple"
                          aria-label="Previous" @click="prevPage">
                          <svg class="w-4 h-4 fill-current" aria-hidden="true" viewBox="0 0 20 20">
                            <path
                              d="M12.707 5.293a1 1 0 010 1.414L9.414 10l3.293 3.293a1 1 0 01-1.414 1.414l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 0z"
                              clip-rule="evenodd" fill-rule="evenodd"></path>
                          </svg>
                        </button>
                      </li>
                      <li>
                        <button class="px-3 py-1 rounded-md focus:outline-none focus:shadow-outline-purple"
                          id="currentPageButton"></button>
                      </li>
                      <li>
                        <button class="px-3 py-1 rounded-md rounded-r-lg focus:outline-none focus:shadow-outline-purple"
                          aria-label="Next" @click="nextPage">
                          <svg class="w-4 h-4 fill-current" aria-hidden="true" viewBox="0 0 20 20">
                            <path
                              d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
                              clip-rule="evenodd" fill-rule="evenodd"></path>
                          </svg>
                        </button>
                      </li>
                    </ul>
                  </nav>
                </span>
              </div>
            </div>
          </div>
        </div>
      </main>
    </div>
  </div>
  <script>
    let clubs = [];
    let members = [];
    let itemsPerPage = 6;
    let currentPage = 1;
    let totalPages = 1;

    async function fetchClubs() {
      try {
        const response = await fetch('/user/clubs');
        clubs = await response.json();
        populateClubSelect();
      } catch (error) {
        console.error(error);
      }
    }

    function populateClubSelect() {
      const clubSelect = document.getElementById('clubSelect');
      const defaultOption = document.createElement('option');
      defaultOption.text = '모임 선택';
      defaultOption.disabled = true;
      defaultOption.selected = true;
      clubSelect.add(defaultOption);

      clubs.forEach(club => {
        const option = document.createElement('option');
        option.text = club.club_name;
        option.value = club.club_id;
        clubSelect.add(option);
      });

      clubSelect.addEventListener('change', fetchMembers);
    }

    async function fetchMembers() {
      const clubId = document.getElementById('clubSelect').value;
      try {
        const response = await fetch(`/clubs/${clubId}/members`);
        members = await response.json(); // members 변수에 할당
        currentPage = 1;
        totalPages = Math.ceil(members.length / itemsPerPage);
        populateMemberTable(members);
      } catch (error) {
        console.error(error);
        return [];
      }
    }

    async function assignLeader() {
      if (selectedMember) {
        const confirmed = confirm(`${selectedMember.User.username} 을(를) 모임장으로 지정하시겠습니까?`);
        if (confirmed) {
          const clubId = document.getElementById('clubSelect').value;
          const memberId = selectedMember.user_id;

          try {
            const response = await fetch(`/clubs/${clubId}/members/${memberId}/assign-leader`, {
              method: 'POST',
              headers: {
                'Content-Type': 'application/json'
              },
              body: JSON.stringify({ cate: 1 }) // cate를 1로 설정하여 모임장으로 지정
            });

            if (response.ok) {
              console.log('모임장으로 지정되었습니다.');
              // 선택된 멤버의 ID를 사용하여 해당 행을 직접 찾아 업데이트
              fetchMembers();
            } else {
              console.error('모임장 지정에 실패했습니다.');
              // 실패 처리 로직
            }
          } catch (error) {
            console.error(error);
            // 오류 처리 로직
          }
        }
      } else {
        console.log('선택된 멤버가 없습니다.');
      }
    }

    function formatDate(date) {
      // 예시: YYYY년 MM월 DD일
      const formattedDate = new Date(date).toLocaleDateString('ko-KR', { year: 'numeric', month: '2-digit', day: '2-digit' });
      return formattedDate;
    }

    function selectMember(member) {
      selectedMember = member;
      console.log('선택된 멤버:', member);
    }

    // 모임 선택 셀렉트 박스
    const clubSelect = document.getElementById('clubSelect');

    let selectedMember = null;
    function selectMember(member) {
      selectedMember = member;
      console.log('선택된 유저:', member);
    }

    function populateMemberTable() {
      const memberTable = document.getElementById('memberTable');
      memberTable.innerHTML = ''; // 테이블 비우기

      const startIndex = (currentPage - 1) * itemsPerPage;
      const endIndex = Math.min(startIndex + itemsPerPage, members.length);
      const currentPageMembers = members.slice(startIndex, endIndex);

      currentPageMembers.forEach(member => {
        const row = document.createElement('tr');

        const profileCell = document.createElement('td');
        profileCell.textContent = member.User.username; // member 객체의 User 속성으로 변경
        profileCell.classList.add('text-center', 'px-4', 'py-3', 'font-medium');
        row.appendChild(profileCell);

        const roleCell = document.createElement('td');
        roleCell.textContent = member.cate === 1 ? '모임장' : '회원';
        roleCell.classList.add('text-center', 'px-4', 'py-3', 'font-sm');
        row.appendChild(roleCell);

        const joinDateCell = document.createElement('td');
        joinDateCell.textContent = formatDate(member.joinDate);
        joinDateCell.classList.add('text-center', 'px-4', 'py-3', 'font-sm');
        row.appendChild(joinDateCell);

        const selectCell = document.createElement('td');
        selectCell.classList.add('flex', 'items-center', 'justify-center', 'px-4', 'py-4');
        const checkbox = document.createElement('input');
        checkbox.type = 'checkbox';
        checkbox.classList.add('form-check', 'text-purple-600', 'hover:text-purple-900', 'focus:outline-none');
        checkbox.addEventListener('change', () => {
          if (checkbox.checked) {
            selectMember(member)
          } else {
            selectedMember = null;
          }
        });
        selectCell.appendChild(checkbox);
        row.appendChild(selectCell);

        memberTable.appendChild(row);

      });
      document.getElementById('currentPageButton').textContent = currentPage;
    }

    function nextPage() {
      if (currentPage < totalPages) {
        currentPage++;
        populateMemberTable();
      }
    }

    function prevPage() {
      if (currentPage > 1) {
        currentPage--;
        populateMemberTable();
      }
    }

    window.onload = async function () {
      await fetchClubs(); // 클럽 목록을 가져오는 함수 호출
    }
  </script>
</body>

</html>